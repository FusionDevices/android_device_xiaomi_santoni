[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group Santoni Maintainers
	owner = group user/Tab Fitts (tabp0le)
[access "refs/for/refs/*"]
	read = group Santoni Maintainers
	read = group user/Tab Fitts (tabp0le)
	push = group Santoni Maintainers
	push = group user/Tab Fitts (tabp0le)
	label-Code-Review = -2..+2 group Santoni Maintainers
	label-Code-Review = -2..+2 group user/Tab Fitts (tabp0le)
	label-Verified = -1..+1 group Santoni Maintainers
	label-Verified = -1..+1 group user/Tab Fitts (tabp0le)
	rebase = group Santoni Maintainers
	rebase = group user/Tab Fitts (tabp0le)
	removeReviewer = group Santoni Maintainers
	removeReviewer = group user/Tab Fitts (tabp0le)
	submit = group Santoni Maintainers
	submit = group user/Tab Fitts (tabp0le)
[access "refs/heads/*"]
	read = group Santoni Maintainers
	read = group user/Tab Fitts (tabp0le)
	create = group Santoni Maintainers
	create = group user/Tab Fitts (tabp0le)
	push = group Santoni Maintainers
	push = +force group user/Tab Fitts (tabp0le)
	pushMerge = group Santoni Maintainers
	pushMerge = group user/Tab Fitts (tabp0le)
[access "refs/tags/*"]
	push = +force group Santoni Maintainers
	push = group user/Tab Fitts (tabp0le)
	push = +force group user/Tab Fitts (tabp0le)
	label-Verified = -1..+1 group Santoni Maintainers
[submit]
	action = rebase if necessary
